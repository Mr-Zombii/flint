@external(c, "flint_stdlib", "assert")
pub fn assert(cond: Bool) Nil

@external(c, "flint_stdlib", "print")
pub fn print(string: String) Nil

pub fn println(v: String) Nil {
    print(v)
    print("\n")
}

pub fn main() Nil {
    mut x: String = "Hi Universe!"
    print(x)
    x = "Hello World!"
    print(x)
}